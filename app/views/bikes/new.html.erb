<h1>VIEW ONDE SERÁ MOSTRADO O FORMULÁRIO PARA INCLUIR UMA BIKE</h1>

<div class="container">
  <div>
    <%= simple_form_for(@bike) do |f| %>
      <%= f.input :address %>
      <%= f.input :description %>
      <div class="d-flex justify-content-between">
        <%= f.submit 'Submit', class: "btn btn-success" %>
      </div>
    <% end %>
  </div>
</div>